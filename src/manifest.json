{
  "manifest_version": 2,
  "name": "Chrome Spoiler Alert",
  "version": "0.1",
  "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  },
  "permissions": ["contextMenus"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["spoiler-alert.js"]
    }
  ]
}
